}
.uni-input {
	height: 50rpx;
	padding: 15rpx 25rpx;
	line-height:50rpx;
	font-size:28rpx;
	background:#FFF;
	flex: 1;
}
radio-group, checkbox-group{
	width:100%;
}
radio-group label, checkbox-group label{
	padding-right:20rpx;
}
.uni-form-item .with-fun{
	display:flex;
	flex-wrap:nowrap;
	background:#FFFFFF;
}
.uni-form-item .with-fun .uni-icon{
	width:40px;
