	padding:10rpx 0;
}
.uni-btn-v button{margin:20rpx 0;}
/* 表单 */
.uni-form-item{
	display:flex;
	width:100%;
	padding:10rpx 0;
}
.uni-form-item .title{
	padding:10rpx 25rpx;
}
.uni-label {
	width: 210rpx;
	word-wrap: break-word;
